#!/usr/bin/env node

const chalk = require("chalk");
const files = process.argv.slice(2);

for (const file of files) {
	console.log(chalk.red(`No linter for ${file}`));
}

console.log(
	"Fix by adding a matching rule or adding an ignore rule. See https://github.com/ahawkins/lint-staged-type for more information."
);

process.exit(1);
